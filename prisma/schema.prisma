generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Credit {
  id           String   @id @default(cuid())
  customerName String
  customerId   String
  amount       Decimal  @db.Decimal(14, 2)
  interestRate Float
  termMonths   Int
  agent        String
  createdAt    DateTime @default(now())
}

model User {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
